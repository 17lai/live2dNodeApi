{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "/"
            }
        },
        {
            "src": "/html/**/*",
            "use": "@vercel/static"
        },
        {
            "src": "/modelList.json",
            "use": "@vercel/static"
        },
        {
            "src": "/preview/*.png",
            "use": "@vercel/static"
        },
        {
            "src": "/main.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/.*",
            "dest": "/main.js"
        }
    ]
}
