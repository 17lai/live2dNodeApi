{
    "name": "Live2d Node Api",
    "version": 2,
    "builds": [
        {
            "src": "/html/**/*",
            "use": "@vercel/static"
        },
        {
            "src": "/modelList.json",
            "use": "@vercel/static"
        },
        {
            "src": "/main.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/.*",
            "dest": "/main.js"
        }
    ]
}
